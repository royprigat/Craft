event move_up(player) {
    condition = key_press("UP");
    action {
        player.pos.y = player.pos.y + 5;
    }
}

element player {
    size = (5,50);
    color = "4268f4";
}

world {
    properties {
        size = (500,500);
        color = "f4f441";
    }

    element player = new player(100,100);
    add_event(move_up);
}