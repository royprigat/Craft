event move_up(player p) {
    condition = key_press("UP");
    action {
        p.pos.y = p.pos.y + 5;
    }
}

element player {
    size = (5,50);
    color = "4268f4";
}

world {
    properties {
        size = (500,500);
        color = "f4f441";
    }

    element player roy = new player(100,100);
    add_event(move_up(roy));
}